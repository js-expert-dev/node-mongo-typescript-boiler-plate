{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "target": "es6",
    "outDir": "dist",
    "baseUrl": "src",
    "paths": {
      "@utils/*": ["utils/*"],
      "@models/*": ["models/*"],
      "@routes/*": ["routes/*"],
      "@services/*": ["services/*"],
      "@controllers/*": ["controllers/*"],
      "@validations/*": ["validations/*"]
    }
  },
  "lib": ["es2015"]
}
